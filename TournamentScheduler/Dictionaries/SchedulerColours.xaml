<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:local="clr-namespace:TournamentScheduler.Dictionaries">



    <Color x:Key="Editor.Window.GrayColour1">#ff1f1f1f</Color>
    <Color x:Key="Editor.Window.GrayColour2">#ff262626</Color>
    <Color x:Key="Editor.Window.GrayColour3">#ff313131</Color>
    <Color x:Key="Editor.Window.GrayColour4">#ff404040</Color>
    <Color x:Key="Editor.Window.GrayColour5">#ff535353</Color>
    <Color x:Key="Editor.Window.GrayColour6">#ff6a6a6a</Color>
    <Color x:Key="Editor.Disabled.FontColour">#ff868686</Color>
    <Color x:Key="Editor.FontColour">#ffdddddd</Color>
    <Color x:Key="Editor.RedColour">#ffff5a6a</Color>
    <Color x:Key="Editor.GreenColour">#ff90ee90</Color>
    <Color x:Key="Editor.BlueColour">#ff80deff</Color>
    <Color x:Key="Editor.OrangeColour">#ffffd067</Color>
    <Color x:Key="Editor.Selected.BackgroundColour">#ff3c67b6</Color>

    <SolidColorBrush Color="{StaticResource Editor.Window.GrayColour1}" x:Key="Editor.Window.GrayBrush1"/>
    <SolidColorBrush Color="{StaticResource Editor.Window.GrayColour2}" x:Key="Editor.Window.GrayBrush2"/>
    <SolidColorBrush Color="{StaticResource Editor.Window.GrayColour3}" x:Key="Editor.Window.GrayBrush3"/>
    <SolidColorBrush Color="{StaticResource Editor.Window.GrayColour4}" x:Key="Editor.Window.GrayBrush4"/>
    <SolidColorBrush Color="{StaticResource Editor.Window.GrayColour5}" x:Key="Editor.Window.GrayBrush5"/>
    <SolidColorBrush Color="{StaticResource Editor.Window.GrayColour6}" x:Key="Editor.Window.GrayBrush6"/>
    <SolidColorBrush Color="{StaticResource Editor.Disabled.FontColour}" x:Key="Editor.Disabled.FontBrush"/>
    <SolidColorBrush Color="{StaticResource Editor.FontColour}" x:Key="Editor.FontBrush"/>
    <SolidColorBrush Color="{StaticResource Editor.RedColour}" x:Key="Editor.RedBrush"/>
    <SolidColorBrush Color="{StaticResource Editor.GreenColour}" x:Key="Editor.GreenBrush"/>
    <SolidColorBrush Color="{StaticResource Editor.BlueColour}" x:Key="Editor.BlueBrush"/>
    <SolidColorBrush Color="{StaticResource Editor.OrangeColour}" x:Key="Editor.OrangeBrush"/>
    <SolidColorBrush Color="{StaticResource Editor.Selected.BackgroundColour}" x:Key="Editor.Selected.BackgroundBrush"/>
</ResourceDictionary>